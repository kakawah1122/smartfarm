        "performance-analysis/performance-analysis"
      ]
    },
    {
      "root": "packageAI",
      "name": "ai",
      "pages": [
        "ai-diagnosis/ai-diagnosis",
        "diagnosis-history/diagnosis-history",
        "weather-detail/weather-detail"
      ]
