.container {
  padding: 40rpx;
  background: #f5f5f5;
  min-height: 100vh;
}

.header {
  background: white;
  padding: 40rpx;
  border-radius: 16rpx;
  margin-bottom: 40rpx;
  text-align: center;
}

.title {
  display: block;
  font-size: 36rpx;
  font-weight: bold;
  color: #333;
  margin-bottom: 16rpx;
}

.subtitle {
  display: block;
  font-size: 28rpx;
  color: #666;
}

.logs {
  background: white;
  border-radius: 16rpx;
  padding: 20rpx;
  margin-bottom: 40rpx;
}

.log-item {
  padding: 20rpx;
  margin: 10rpx 0;
  border-radius: 8rpx;
  border-left: 6rpx solid #ccc;
}

.log-info {
  background: #f0f9ff;
  border-left-color: #0ea5e9;
}

.log-success {
  background: #f0fdf4;
  border-left-color: #22c55e;
}

.log-warn {
  background: #fffbeb;
  border-left-color: #f59e0b;
}

.log-error {
  background: #fef2f2;
  border-left-color: #ef4444;
}

.log-title {
  background: #fafafa;
  border-left-color: #666;
  font-weight: bold;
}

.log-time {
  display: block;
  font-size: 24rpx;
  color: #999;
  margin-bottom: 8rpx;
}

.log-message {
  display: block;
  font-size: 28rpx;
  color: #333;
  word-break: break-all;
}

.footer {
  background: #fffbeb;
  padding: 32rpx;
  border-radius: 16rpx;
  border: 2rpx solid #f59e0b;
}

.tip {
  display: block;
  font-size: 26rpx;
  color: #92400e;
  line-height: 1.6;
  margin: 8rpx 0;
}

